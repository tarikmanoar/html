				var counter = 5;
				var id = setInterval(function() {
				   counter--;
				   if(counter > 0) {
						var msg = "You can skip ad in " + counter + "s";
						$("#notice").text(msg);
				   } else {
						$("#notice").hide();
						$("#hidead").show();
						clearInterval(id);
				  }
				}, 1000);

				$("#hidead").click(function() {
				$("#loading-ad").hide();
				});

				});
